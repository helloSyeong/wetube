extends layouts/main.pug
include mixins/videoBlock

block content
  p Search
  .search__header
    h3 Searching for: #{searchingBy}
  .serarch__videos
    if videos.length === 0
      h5 No Video Found
    each item in videos
      +videoBlock({
        id:item.id,
        title:item.title,
        views:item.views,
        videoFile:item.videoFile
      })